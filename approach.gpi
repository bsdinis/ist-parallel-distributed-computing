set terminal postscript eps enhanced color font 'Helvetica' # size 9.00, 6.00
set output 'approach.eps'

set key outside above

set grid
set border linewidth 2

plot 'only.dat' using 1:2 with linespoints lt 1 lw 2   title "Tree", 'ours.dat' using 1:2 with linespoints lt 2 lw 2    title "Tree + Projection", 'worst.dat' using 1:2 with linespoints lt 3 lw 2    title "All";
